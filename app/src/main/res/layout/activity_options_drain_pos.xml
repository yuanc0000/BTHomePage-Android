<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".OptionsDrainPos">

    <TextView
        android:id="@+id/textView31"
        android:layout_width="370dp"
        android:layout_height="67dp"
        android:layout_marginTop="50dp"
        android:gravity="center"
        android:text="Specific Positions"
        android:textColor="#000000"
        android:textSize="35sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button8"
        android:layout_width="150dp"
        android:layout_height="82dp"
        android:layout_marginTop="48dp"
        android:onClick="sendMessagesFrontLungs"
        android:text="Front of Lungs"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.197"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />

    <Button
        android:id="@+id/button9"
        android:layout_width="150dp"
        android:layout_height="82dp"
        android:layout_marginTop="48dp"
        android:onClick="sendMessagesSideLungs"
        android:text="Sides of lungs"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.836"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView31" />


    <Button
        android:id="@+id/button11"
        android:layout_width="150dp"
        android:layout_height="82dp"
        android:layout_marginTop="37dp"
        android:onClick="sendMessagesBackLungs"
        android:text="Back of lungs"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button8" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="240dp"
        android:onClick="sendMessagesExercises"
        android:text="Back to Exercises"
        android:textColor="#000000"
        app:backgroundTint="#C5DEEA"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button11" />


</androidx.constraintlayout.widget.ConstraintLayout>